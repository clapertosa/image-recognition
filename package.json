{
  "name": "image-recognition",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "version": "1.0.0",
  "description": "Image recognition to detect objects and much more",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "start:dev": "concurrently \"npm run webpack\" \" npm run server\"",
    "build": "rimraf dist && webpack --config webpack.config.production.js --progress --profile --colors",
    "heroku-postbuild": "npm run build",
    "server": "nodemon ./server.js",
    "webpack": "webpack --config webpack.config.js -w --inline --progress --profile --colors",
    "test": "jest"
  },
  "keywords": [
    "image",
    "recognition",
    "cloudmersive"
  ],
  "author": "Claudio Lapertosa",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^3.0.1",
    "@material-ui/icons": "^3.0.1",
    "@sendgrid/mail": "^6.3.1",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "knex": "^0.15.2",
    "morgan": "^1.9.0",
    "multer": "^1.3.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.4.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.1.3",
    "babel-loader": "^8.0.0",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
